{
  "useTabs": true,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 100,
  "semi": true,
  "plugins": [
    "prettier-plugin-svelte",
    "prettier-plugin-astro"
  ],
  "overrides": [
    {
      "files": [
        "*.svelte",
        "*.astro"],
      "options": {
        "parser": "astro"
      }
    }
  ]
}
